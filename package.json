    "@types/jasmine": "~5.1.0",
    "@types/jest": "^30.0.0",
    "jasmine-core": "~5.9.0",
    "jest": "^30.2.0",
    "jest-preset-angular": "^15.0.3",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nx": "^22.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
